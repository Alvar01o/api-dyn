#DB_PORT=3306
DB_PORT=5432
DB_NAME=apidyn
DB_USER=dev
DB_PASSWORD=devpass

# Limite máximo de tamaño (bytes)
MAX_SCHEMA_FILE_SIZE=0   # 0 = ilimitado

# Eliminar schema luego de aplicar exitosamente
DELETE_SCHEMA_AFTER_APPLY=true

# Ruta temporal
SCHEMA_TMP_PATH=/tmp/schemas

# Upload / seguridad
MAX_SCHEMA_FILE_SIZE=0                 # 0 = ilimitado (pero recomendado setear)
SCHEMA_TMP_PATH=/tmp/schemas

# Comportamiento
DELETE_SCHEMA_AFTER_APPLY=true         # borra archivo solo si todo OK
SCHEMA_DROP_DB_ON_FAILURE=true         # si falla apply, dropea la DB creada
SCHEMA_USE_TRANSACTION=true            # envuelve apply en TX (si tu .sql no trae BEGIN/COMMIT)
SCHEMA_STATEMENT_TIMEOUT_MS=60000      # postgres
SCHEMA_CONNECT_TIMEOUT_MS=10000        # postgres

# Opcional: deshabilitar creación si ya existe
SCHEMA_FAIL_IF_DB_EXISTS=true

# Seguridad
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=10

# SQL validation
SCHEMA_ENABLE_SQL_GUARD=true
SCHEMA_FORBIDDEN_KEYWORDS=DROP DATABASE,USE ,GRANT ,REVOKE ,ALTER USER
SCHEMA_DRY_RUN=false

# Idempotencia
SCHEMA_ENABLE_IDEMPOTENCY=true

# Job storage
SCHEMA_JOB_STORE_PATH=/tmp/schema-jobs

